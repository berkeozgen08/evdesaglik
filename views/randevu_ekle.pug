extends layout.pug

block content
	h1 Randevu Ekle
	form(action="/randevu_ekle" method="post")
		label(for="hasta_tc") Hasta TC
		input(type="text" id="hasta_tc" name="hasta_tc")
		label(for="doktor_tc") Doktor TC
		input(type="text" id="doktor_tc" name="doktor_tc")
		label(for="tarih") Tarih
		input(type="text" id="tarih" name="tarih" placeholder="aa-gg-yyyy 00:00:00")
		input(type="submit" value="Ekle")
